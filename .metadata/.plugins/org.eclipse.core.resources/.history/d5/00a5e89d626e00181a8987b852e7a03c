		}
		for(int i = 0; i < whereTrans.size(); i++) {
			transStream.println(whereTrans.get(i));
		}
		
		for(int i = 0; i < whereTrans1.size(); i++) {
			transAllStream.println(whereTrans1.get(i));
		}

		System.out.println(hits);
		System.out.println(hits_5G);
