					hits_5G++;
				}
				if(pimAll4.get(x) > list3.get(q) && pimAll4.get(x) < list3.get(q+1)) {
					whereTrans1.add("TX: " + pimAll4.get(x) + "-" + pimAll4.get(x+1) + "  RX: " + list3.get(q) + "-" + list3.get(q+1));
					hits_5G++;
				}
				//+ - - Pattern hit checks
				if(pimAll5.get(x+1) < list3.get(q+1) && pimAll5.get(x+1) > list3.get(q)) {
					whereTrans1.add("TX: " + pimAll5.get(x) + "-" + pimAll5.get(x+1) + "  RX: " + list3.get(q) + "-" + list3.get(q+1));
					hits_5G++;
				}
				if(pimAll5.get(x) > list3.get(q) && pimAll5.get(x) < list3.get(q+1)) {
					whereTrans1.add("TX: " + pimAll5.get(x) + "-" + pimAll5.get(x+1) + "  RX: " + list3.get(q) + "-" + list3.get(q+1));
					hits_5G++;
				}
				//+ + - Pattern hit checks
				if(pimAll6.get(x+1) < list3.get(q+1) && pimAll6.get(x+1) > list3.get(q)) {
					whereTrans1.add("TX: " + pimAll6.get(x) + "-" + pimAll6.get(x+1) + "  RX: " + list3.get(q) + "-" + list3.get(q+1));
					hits_5G++;
				}
				if(pimAll6.get(x) > list3.get(q) && pimAll6.get(x) < list3.get(q+1)) {
					whereTrans1.add("TX: " + pimAll6.get(x) + "-" + pimAll6.get(x+1) + "  RX: " + list3.get(q) + "-" + list3.get(q+1));
					hits_5G++;
				}
			}
		}
		
		//Write list to file
		//5Gen 1
		PrintStream pimStream1 = null;
		PrintStream pimStream2 = null;
		PrintStream pimStream3 = null;
		PrintStream pimStream4 = null;
		PrintStream pimStream5 = null;
		PrintStream pimStream6 = null;
