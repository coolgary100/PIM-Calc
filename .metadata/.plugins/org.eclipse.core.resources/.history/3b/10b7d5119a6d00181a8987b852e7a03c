import java.lang.reflect.Array;
import java.util.Arrays;

public class PIMCalc {
	public static void main(String[] args) {		
		double[] trans1 = new double[2];
		double[] trans2 = new double[2];
		double[] trans3 = new double[2];
		double[] rcv1 = new double[2];
		double[] rcv2 = new double[2];
		double[] rcv3 = new double[2];
		
		trans1[0] = 870;
		trans1[1] = 880;
		trans2[0] = 890;
		trans2[1] = 891.5;
		trans3[0] = 891.5;
		trans3[1] = 894;
		rcv1[0] = 825;
		rcv1[1] = 835;
		rcv2[0] = 845;
		rcv2[1] = 846.5;
		rcv3[0] = 846.5;
		rcv3[1] = 849;
		
		int hits = 0;
		
		double[] pim1 = new double[2];
		double[] pim2 = new double[2];
		
		trans1[0] = trans1[0]*2;
		trans1[1] = trans1[1]*2;
		pim1[0] = trans1[0] - trans2[0];
		pim1[1] = trans1[1] - trans2[1];
		pim2[0] = trans1[0] - trans3[0];
		pim2[1] = trans1[1] - trans3[1];
		
		System.out.println(Arrays.toString(trans1));
		System.out.println(Arrays.toString(trans2));
		System.out.println(Arrays.toString(trans3));
	}
}
