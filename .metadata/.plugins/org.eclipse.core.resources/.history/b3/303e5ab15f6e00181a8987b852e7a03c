import java.io.*;
import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class PIMCalc {
	public static void main(String[] args) {
		
		//Variables to calculate Intermod for all permutations with at least one value being 5G frequency
		//Checking against all receiver frequencies
		double[] gen5_1 = new double[2];
		double[] gen5_2 = new double[2];
		double[] gen5_3 = new double[2];
		double[] gen5_4 = new double[2];
		double tempPimLow1 = 0;
		double tempPimHigh1 = 0;
		double tempPimLow2 = 0;
		double tempPimHigh2 = 0;
		double tempPimLow3 = 0;
		double tempPimHigh3 = 0;
		double tempPimLow4 = 0;
		double tempPimHigh4 = 0;
		
		gen5_1[0] = 3575;
		gen5_1[1] = 3600;
		gen5_2[0] = 3600;
		gen5_2[1] = 3625;
		gen5_3[0] = 3525;
		gen5_3[1] = 3550;
		gen5_4[0] = 3550;
		gen5_4[1] = 3575;
		
		//5Gen 1
		ArrayList<Double> PIM1 = new ArrayList<Double>(); 
		ArrayList<Double> PIM2 = new ArrayList<Double>();
		ArrayList<Double> PIM3 = new ArrayList<Double>();
		
		//5Gen 2
		ArrayList<Double> PIM5 = new ArrayList<Double>();
		ArrayList<Double> PIM6 = new ArrayList<Double>();
		ArrayList<Double> PIM7 = new ArrayList<Double>();
		
		//5Gen 3
		ArrayList<Double> PIM9 = new ArrayList<Double>();
		ArrayList<Double> PIM10 = new ArrayList<Double>();
		ArrayList<Double> PIM11 = new ArrayList<Double>();
		
		//5Gen 4
		ArrayList<Double> PIM13 = new ArrayList<Double>();
		ArrayList<Double> PIM14 = new ArrayList<Double>();
		ArrayList<Double> PIM15 = new ArrayList<Double>();
		
		//Index Location of Files
		List<String> whereTrans = new ArrayList<String>();
//		List<String> whereRcv = new ArrayList<String>();
//		List<String> whichFile = new ArrayList<String>();
		
		//End of Variables 
		
		//Variables for Calculating Intermod over all frequency permutations. Only checking against 5G receiver frequencies
		//The old calculator already included every permutation for other frequencies
		
		double tempPimLow5 = 0;
		double tempPimHigh5 = 0;
		
		//PIM for checking against all Rx frequencies
		ArrayList<Double> pimAll1 = new ArrayList<Double>(); 
		ArrayList<Double> pimAll2 = new ArrayList<Double>();
		ArrayList<Double> pimAll3 = new ArrayList<Double>();
		ArrayList<Double> pimAll4 = new ArrayList<Double>();
		
		
		//Transmission and Receiver frequency files
		List<Double> list = new ArrayList<Double>();
		List<Double> list2 = new ArrayList<Double>();
		
		//File that contains value of TX and RX intersection
		List<String> whereTrans1 = new ArrayList<String>();
		
		//Grabs the appropriate text files
		File file = new File("Transmission.txt");
		File file2 = new File("Receiver.txt");
		
		BufferedReader reader = null;
		BufferedReader reader2 = null;
		
		try (FileInputStream fis = new FileInputStream(file)) {
			reader = new BufferedReader(new FileReader(file));
			String text = null;
			
			while((text = reader.readLine()) != null) {
				list.add(Double.parseDouble(text));
			}
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			try {
				if (reader != null) {
					reader.close();
				}
			} catch (IOException e) {
				
			}
		}
		
		try (FileInputStream fis = new FileInputStream(file2)) {
			reader2 = new BufferedReader(new FileReader(file2));
			String text2 = null;
			
			while((text2 = reader2.readLine()) != null) {
				list2.add(Double.parseDouble(text2));
			}
		} catch (IOException e) {
			e.printStackTrace();
		} finally {
			try {
				if (reader2 != null) {
					reader2.close();
				}
			} catch (IOException e) {
				
			}
		}
		
		int hits = 0;

		//System.out.println(list);
		//System.out.println(list2);

		//Calculate the 3rd Order Intermod with minimum one 5G frequency
		//+ + +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j=j+2) {
				//5Gen 1
				tempPimLow1 = list.get(i) + list.get(j) + gen5_1[0];
				tempPimHigh1 = list.get(i+1) + list.get(j+1) + gen5_1[1];
				//5Gen 2
				tempPimLow2 = list.get(i) + list.get(j) + gen5_2[0];
				tempPimHigh2 = list.get(i+1) + list.get(j+1) + gen5_2[1];
				//5Gen 3
				tempPimLow3 = list.get(i) + list.get(j) + gen5_3[0];
				tempPimHigh3 = list.get(i+1) + list.get(j+1) + gen5_3[1];
				//5Gen 4
				tempPimLow3 = list.get(i) + list.get(j) + gen5_4[0];
				tempPimHigh3 = list.get(i+1) + list.get(j+1) + gen5_4[1];
				//Push to the PIMs
				PIM1.add(tempPimLow1);
				PIM5.add(tempPimLow2);
				PIM9.add(tempPimLow3);
				PIM13.add(tempPimLow4);
				PIM1.add(tempPimHigh1);
				PIM5.add(tempPimHigh2);
				PIM9.add(tempPimHigh3);
				PIM13.add(tempPimHigh4);
			}
		}
		
		//- + +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j=j+2) {
				//5Gen 1
				tempPimLow1 = -list.get(i+1) + list.get(j) + gen5_1[0];
				tempPimHigh1 = -list.get(i) + list.get(j+1) + gen5_1[1];
				//5Gen 2
				tempPimLow2 = -list.get(i+1) + list.get(j) + gen5_2[0];
				tempPimHigh2 = -list.get(i) + list.get(j+1) + gen5_2[1];
				//5Gen 3
				tempPimLow3 = -list.get(i+1) + list.get(j) + gen5_3[0];
				tempPimHigh3 = -list.get(i) + list.get(j+1) + gen5_3[1];
				//5Gen 4
				tempPimLow4 = -list.get(i+1) + list.get(j) + gen5_4[0];
				tempPimHigh4 = -list.get(i) + list.get(j+1) + gen5_4[1];
				//Push to Pims
				PIM2.add(tempPimLow1);
				PIM6.add(tempPimLow2);
				PIM10.add(tempPimLow3);
				PIM14.add(tempPimLow4);
				PIM2.add(tempPimHigh1);
				PIM6.add(tempPimHigh2);
				PIM10.add(tempPimHigh3);
				PIM14.add(tempPimHigh4);
			}
		}
		
		// - - +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j=j+2) {
				//5Gen 1
				tempPimLow1 = -list.get(i+1) - list.get(j+1) + gen5_1[0];
				tempPimHigh1 = -list.get(i) - list.get(j) + gen5_1[1];
				//5Gen 2
				tempPimLow2 = -list.get(i+1) - list.get(j+1) + gen5_2[0];
				tempPimHigh2 = -list.get(i) - list.get(j) + gen5_2[1];
				//5Gen 3
				tempPimLow3 = -list.get(i+1) - list.get(j+1) + gen5_3[0];
				tempPimHigh3 = -list.get(i) - list.get(j) + gen5_3[1];
				//5Gen 4
				tempPimLow4 = -list.get(i+1) - list.get(j+1) + gen5_4[0];
				tempPimHigh4 = -list.get(i) - list.get(j) + gen5_4[1];
				//Push to Pim
				PIM3.add(tempPimLow1);
				PIM7.add(tempPimLow2);
				PIM11.add(tempPimLow3);
				PIM15.add(tempPimLow4);
				PIM3.add(tempPimHigh1);
				PIM7.add(tempPimHigh2);
				PIM11.add(tempPimHigh3);
				PIM15.add(tempPimHigh4);
			}
		}
		
		//Calculate 3rd order Intermod using all frequencies 
		//+ + +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j = j+2) {
				for(int k = j; k < list.size(); k = k+2) {
					tempPimLow5 = list.get(i) + list.get(j) + list.get(k);
					tempPimHigh5 = list.get(i+1) + list.get(j+1) + list.get(k+1);
					pimAll1.add(tempPimLow5);
					pimAll1.add(tempPimHigh5);
				}
			}
		}
		
		//- + +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j = j+2) {
				for(int k = j; k < list.size(); k = k=k+2) {
					tempPimLow5 = list.get(i+1) + list.get(j) + list.get(k);
					tempPimHigh5 = list.get(i) + list.get(j+1) + list.get(k+1);
					pimAll2.add(tempPimLow5);
					pimAll2.add(tempPimHigh5);
				}
			}
		}
		
		//- - +
		for(int i = 0; i < list.size(); i = i+2) {
			for(int j = i; j < list.size(); j = j+2) {
				for(int k = j; k < list.size(); k = k+2) {
					tempPimLow5 = list.get(i+1) + list.get(j+1) + list.get(k);
					tempPimHigh5 = list.get(i) + list.get(j) + list.get(k+1);
					pimAll3.add(tempPimLow5);
					pimAll3.add(tempPimHigh5);
				}
			}
		}
		
		//Checking for hits with minimum 1 5G
		for(int k = 0; k < list2.size(); k = k+2) {
			for(int a = 0; a < list2.size(); a = a+2) {
				//5Gen 1
				if((PIM1.get(k) == list2.get(a))
						|| (PIM1.get(k) == list2.get(a+1))
						|| (PIM1.get(k+1) == list2.get(a))
						|| (PIM1.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM2.get(k) == list2.get(a))
						|| (PIM2.get(k) == list2.get(a+1))
						|| (PIM2.get(k+1) == list2.get(a))
						|| (PIM2.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM3.get(k) == list2.get(a))
						|| (PIM3.get(k) == list2.get(a+1))
						|| (PIM3.get(k+1) == list2.get(a))
						|| (PIM3.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				//5Gen 2
				if((PIM5.get(k) == list2.get(a))
						|| (PIM5.get(k) == list2.get(a+1))
						|| (PIM5.get(k+1) == list2.get(a))
						|| (PIM5.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM6.get(k) == list2.get(a))
						|| (PIM6.get(k) == list2.get(a+1))
						|| (PIM6.get(k+1) == list2.get(a))
						|| (PIM6.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM7.get(k) == list2.get(a))
						|| (PIM7.get(k) == list2.get(a+1))
						|| (PIM7.get(k+1) == list2.get(a))
						|| (PIM7.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				//5Gen 3
				if((PIM9.get(k) == list2.get(a))
						|| (PIM9.get(k) == list2.get(a+1))
						|| (PIM9.get(k+1) == list2.get(a))
						|| (PIM9.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM10.get(k) == list2.get(a))
						|| (PIM10.get(k) == list2.get(a+1))
						|| (PIM10.get(k+1) == list2.get(a))
						|| (PIM10.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM11.get(k) == list2.get(a))
						|| (PIM11.get(k) == list2.get(a+1))
						|| (PIM11.get(k+1) == list2.get(a))
						|| (PIM11.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				//5Gen 4
				if((PIM13.get(k) == list2.get(a))
						|| (PIM13.get(k) == list2.get(a+1))
						|| (PIM13.get(k+1) == list2.get(a))
						|| (PIM13.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM14.get(k) == list2.get(a))
						|| (PIM14.get(k) == list2.get(a+1))
						|| (PIM14.get(k+1) == list2.get(a))
						|| (PIM14.get(k+1) == list2.get(a+1))) {
					hits++;
				}
				if((PIM15.get(k) == list2.get(a))
						|| (PIM15.get(k) == list2.get(a+1))
						|| (PIM15.get(k+1) == list2.get(a))
						|| (PIM15.get(k+1) == list2.get(a+1))) {
					hits++;
				}
			}
		}
		
		for(int x = 0; x < list2.size(); x = x+2) {
			for(int q = 0; q < list2.size(); q = q+2) {
				//5Gen 1
				if(PIM1.get(x+1) < list2.get(q+1) && PIM1.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM1.get(x) + "-" + PIM1.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM1");
					hits++;
				}
				if(PIM1.get(x) > list2.get(q) && PIM1.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM1.get(x) + "-" + PIM1.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM1");
					hits++;
				}
				if(PIM2.get(x+1) < list2.get(q+1) && PIM2.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM2.get(x) + "-" + PIM2.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM2");
					hits++;
				}
				if(PIM2.get(x) > list2.get(q) && PIM2.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM2.get(x) + "-" + PIM2.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM2");
					hits++;
				}
				if(PIM3.get(x+1) < list2.get(q+1) && PIM3.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM3.get(x) + "-" + PIM3.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM3");
					hits++;
				}
				if(PIM3.get(x) > list2.get(q) && PIM3.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM3.get(x) + "-" + PIM3.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM3");
					hits++;
				}
				//5Gen 2
				if(PIM5.get(x+1) < list2.get(q+1) && PIM5.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM5.get(x) + "-" + PIM5.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
					//whereRcv.add((double) q);
					//whichFile.add("PIM5");
					hits++;
				}
				if(PIM5.get(x) > list2.get(q) && PIM5.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM5.get(x) + "-" + PIM5.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM5");
					hits++;
				}
				if(PIM6.get(x+1) < list2.get(q+1) && PIM6.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM6.get(x) + "-" + PIM6.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM6");
					hits++;
				}
				if(PIM6.get(x) > list2.get(q) && PIM6.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM6.get(x) + "-" + PIM6.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM6");
					hits++;
				}
				if(PIM7.get(x+1) < list2.get(q+1) && PIM7.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM7.get(x) + "-" + PIM7.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM7");
					hits++;
				}
				if(PIM7.get(x) > list2.get(q) && PIM7.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM7.get(x) + "-" + PIM7.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM7");
					hits++;
				}
				//5Gen 3
				if(PIM9.get(x+1) < list2.get(q+1) && PIM9.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM9.get(x) + "-" + PIM9.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM9");
					hits++;
				}
				if(PIM9.get(x) > list2.get(q) && PIM9.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM9.get(x) + "-" + PIM9.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM9");
					hits++;
				}
				if(PIM10.get(x+1) < list2.get(q+1) && PIM10.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM10.get(x) + "-" + PIM10.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM10");
					hits++;
				}
				if(PIM10.get(x) > list2.get(q) && PIM10.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM10.get(x) + "-" + PIM10.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM10");
					hits++;
				}
				if(PIM11.get(x+1) < list2.get(q+1) && PIM11.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM11.get(x) + "-" + PIM11.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM11");
					hits++;
				}
				if(PIM11.get(x) > list2.get(q) && PIM11.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM11.get(x) + "-" + PIM11.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
