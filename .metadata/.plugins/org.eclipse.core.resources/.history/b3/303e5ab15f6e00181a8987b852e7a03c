//					whichFile.add("PIM11");
					hits++;
				}
				//5Gen 4
				if(PIM13.get(x+1) < list2.get(q+1) && PIM13.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM13.get(x) + "-" + PIM13.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM13");
					hits++;
				}
				if(PIM13.get(x) > list2.get(q) && PIM13.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM13.get(x) + "-" + PIM13.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM13");
					hits++;
				}
				if(PIM14.get(x+1) < list2.get(q+1) && PIM14.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM14.get(x) + "-" + PIM14.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM14");
					hits++;
				}
				if(PIM14.get(x) > list2.get(q) && PIM14.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM14.get(x) + "-" + PIM14.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM14");
					hits++;
				}
				if(PIM15.get(x+1) < list2.get(q+1) && PIM15.get(x+1) > list2.get(q)) {
					whereTrans.add("TX: " + PIM15.get(x) + "-" + PIM15.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM15");
					hits++;
				}
				if(PIM15.get(x) > list2.get(q) && PIM15.get(x) < list2.get(q+1)) {
					whereTrans.add("TX: " + PIM15.get(x) + "-" + PIM15.get(x+1) + "  RX: " + list2.get(q) + "-" + list2.get(q+1));
//					whereRcv.add((double) q);
//					whichFile.add("PIM15");
					hits++;
				}
			}
		}
		
		//Checking for hits with minimum 1 5G
				for(int k = 0; k < list2.size(); k = k+2) {
					for(int a = 0; a < list2.size(); a = a+2) {
						if((pimAll1.get(k) == list2.get(a))
								|| (pimAll1.get(k) == list2.get(a+1))
								|| (pimAll1.get(k+1) == list2.get(a))
								|| (pimAll1.get(k+1) == list2.get(a+1))) {
							hits++;
						}
						if((pimAll2.get(k) == list2.get(a))
								|| (pimAll2.get(k) == list2.get(a+1))
								|| (pimAll2.get(k+1) == list2.get(a))
								|| (pimAll2.get(k+1) == list2.get(a+1))) {
							hits++;
						}
						if((pimAll3.get(k) == list2.get(a))
								|| (pimAll3.get(k) == list2.get(a+1))
								|| (pimAll3.get(k+1) == list2.get(a))
								|| (pimAll3.get(k+1) == list2.get(a+1))) {
							hits++;
						}
					}
				}
				
//				for(int x = 0; x < list2.size(); x = x+2) {
//					for(int q = 0; q < list2.size(); q = q+2) {
